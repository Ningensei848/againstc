{
  "name": "archives-for-open-letters",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "yarn start",
    "start": "yarn emit && docusaurus start",
    "build": "yarn emit && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "emit": "swc src/plugins -d src --config-file src/plugins/.swcrc",
    "typecheck": "tsc",
    "lint": "eslint --cache --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix && yarn format",
    "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}'"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.15",
    "@docusaurus/plugin-pwa": "^2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/react": "^1.6.21",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "@remark-embedder/core": "^2.0.0",
    "@remark-embedder/transformer-oembed": "^2.0.0",
    "axios": "^0.26.0",
    "clsx": "^1.1.1",
    "docusaurus-plugin-google-adsense": "^0.3.1",
    "next-share": "^0.13.0",
    "prism-react-renderer": "^1.2.1",
    "query-string": "^7.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.27.0",
    "react-virtuoso": "^2.7.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.15",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.141",
    "@swc/helpers": "^0.3.3",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "embeddedLanguageFormatting": "off",
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "eslintIgnore": [
    ".eslintrc.js",
    "sidebars.js",
    "*.config.js"
  ]
}
